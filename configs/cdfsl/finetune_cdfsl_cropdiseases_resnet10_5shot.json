{
  "name": "finetune_cdfsl_cropdiseases_resnet10_5shot",
  "model": {
    "name": "finetuning",
    "backbone": "resnet10",
    "transform_train": "wrn_finetune_train",
    "transform_val": "wrn_val",
    "transform_test": "wrn_val"
  },

  "ngpu": 1,
  "random_seed": 42,
  "batch_size": 1,
  "target_loss": "val_accuracy",

  "lr": 0.001,
  "min_lr_decay": 0.0001,
  "weight_decay": 0.0005,
  "patience": 10,
  "max_epoch": 100,

  "train_iters": 600,
  "val_iters": 600,
  "test_iters": 600,
  "tasks_per_batch": 1,

  "dropout": 0.0,
  "avgpool": true,
  "n_classes": 64,
  "collate_fn": "identity",

  "dataset_train": "episodic_cdfsl_cropdiseases",
  "dataset_train_root": "datasets/CDFSL/CropDiseases",
  "classes_train": 5,
  "support_size_train": 5,
  "query_size_train": 15,
  "unlabeled_size_train": 0,

  "dataset_val": "episodic_cdfsl_cropdiseases",
  "dataset_val_root": "datasets/CDFSL/CropDiseases",
  "classes_val": 5,
  "support_size_val": 5,
  "query_size_val": 15,
  "unlabeled_size_val": 0,

  "dataset_test": "episodic_cdfsl_cropdiseases",
  "dataset_test_root": "datasets/CDFSL/CropDiseases",
  "classes_test": 5,
  "support_size_test": 5,
  "query_size_test": 15,
  "unlabeled_size_test": 0,

  "embedding_prop": true,
  "few_shot_weight": 1.0,
  "classification_weight": 0.0,
  "acpl_weight": 0.8,
  "rotation_weight": 0,
  "distance_type": "labelprop",
  "rotation_labels": [0]
}
